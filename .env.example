DEEPSEEK_API_KEY=sk-your-key-here

# Optional: cookies injected into HTTP + browser sampling (semicolon-delimited, e.g. "key=value; key2=value2")
WILEY_COOKIES=
OXFORD_COOKIES=
SCIENCEDIRECT_COOKIES=
CHICAGO_COOKIES=
INFORMS_COOKIES=
NBER_COOKIES=

# Optional: Playwright fingerprint overrides
# Run `uv run econ-atlas samples scd-session warmup --profile-dir .cache/econ-atlas/scd-profile`
# to populate the profile/localStorage entries below.
SCIENCEDIRECT_USER_DATA_DIR=
SCIENCEDIRECT_BROWSER_INIT_SCRIPT=
SCIENCEDIRECT_BROWSER_LOCAL_STORAGE=
SCIENCEDIRECT_BROWSER_HEADLESS=true

# Optional: override browser UA + headers (headers expect JSON, e.g. {"Accept-Language": "..."} )
WILEY_BROWSER_USER_AGENT=
WILEY_BROWSER_HEADERS=
OXFORD_BROWSER_USER_AGENT=
OXFORD_BROWSER_HEADERS=
SCIENCEDIRECT_BROWSER_USER_AGENT=
SCIENCEDIRECT_BROWSER_HEADERS=
# Optional: override the underlying Chrome build Playwright launches (set at most one per source)
WILEY_BROWSER_CHANNEL=
WILEY_BROWSER_EXECUTABLE=
OXFORD_BROWSER_CHANNEL=
OXFORD_BROWSER_EXECUTABLE=
SCIENCEDIRECT_BROWSER_CHANNEL=
SCIENCEDIRECT_BROWSER_EXECUTABLE=
CHICAGO_BROWSER_CHANNEL=
CHICAGO_BROWSER_EXECUTABLE=
INFORMS_BROWSER_CHANNEL=
INFORMS_BROWSER_EXECUTABLE=
NBER_BROWSER_CHANNEL=
NBER_BROWSER_EXECUTABLE=
CHICAGO_BROWSER_USER_AGENT=
CHICAGO_BROWSER_HEADERS=
INFORMS_BROWSER_USER_AGENT=
INFORMS_BROWSER_HEADERS=
NBER_BROWSER_USER_AGENT=
NBER_BROWSER_HEADERS=

# Optional: HTTP auth credentials for browser sampling
WILEY_BROWSER_USERNAME=
WILEY_BROWSER_PASSWORD=
OXFORD_BROWSER_USERNAME=
OXFORD_BROWSER_PASSWORD=
SCIENCEDIRECT_BROWSER_USERNAME=
SCIENCEDIRECT_BROWSER_PASSWORD=
CHICAGO_BROWSER_USERNAME=
CHICAGO_BROWSER_PASSWORD=
INFORMS_BROWSER_USERNAME=
INFORMS_BROWSER_PASSWORD=
